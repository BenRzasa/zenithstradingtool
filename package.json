    "dev": "generate-ore-icons && generate-charm-icons && vite",
    "build": "generate-ore-icons && generate-charm-icons && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "bump:page": "npm version major --no-git-tag-version",
